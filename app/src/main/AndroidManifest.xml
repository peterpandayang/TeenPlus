<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bingkunyang.teenplus">

    <uses-permission android:name="android.permission.INTERNET"/>n

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" android:label="Login Page">
        </activity>
        <activity android:name=".ResetPasswordActivity" android:label="Reset Password Page">
        </activity>
        <activity android:name=".HomeActivity" android:label="Home Page">
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />
</manifest>